<div class="container">


  {{#each data}}
  <div class="panel panel-default">
    <p id={{this.id}}><a href={{this.link}}>{{this.title}}</a></p>
    <button class="addNote btn btn-default" data-id={{this.id}}>Add a note</button>
  </div>
  {{/each}}
  <div id="modal" style="display:none">
    <p id="note-title" data-id=""></p>
    <input id="note-content"></input>
    <button class=" btn btn-default" id="note-button">add a note</button>
    <p>Notes</p>

    <div id="savedNotes"></div>
    <button class="close btn btn-default">Close</button>

  </div>
</div>
<div id="closemodal" style="display:none">
  <p>Success! You have deleted the note</p>
   <button class="deleted-success btn btn-default" id="note-button">ok</button>

  </div>
</div>



<!-- Modal -->


<script src="/js/app.js"></script>